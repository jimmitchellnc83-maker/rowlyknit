{
  "name": "rowly-backend",
  "version": "1.0.0",
  "description": "Rowly Knitting App - Production Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/src/server.js",
    "migrate": "knex --knexfile dist/knexfile.js migrate:latest",
    "migrate:rollback": "knex --knexfile dist/knexfile.js migrate:rollback",
    "seed": "knex --knexfile dist/knexfile.js seed:run",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "rate-limit-redis": "^4.2.0",
    "pg": "^8.11.3",
    "knex": "^3.1.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "cookie-parser": "^1.4.6",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1",
    "pdf-lib": "^1.17.1",
    "validator": "^13.11.0",
    "express-validator": "^7.0.1",
    "nodemailer": "^6.9.7",
    "ioredis": "^5.3.2",
    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "csurf": "^1.11.0",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "joi": "^17.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "prom-client": "^15.1.0",
    "socket.io": "^4.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/validator": "^13.11.7",
    "@types/nodemailer": "^6.4.14",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/uuid": "^9.0.7",
    "@types/csurf": "^1.11.5",
    "@types/express-session": "^1.17.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/socket.io": "^3.0.2",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
